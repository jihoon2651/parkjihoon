<script type="text/javascript">
	/* 3개의 변수활용해서 사칙연산 및 나머지 연산자를 구하시오  */
	
	var num = 2;
	var num2 = 4;
	var result = 0;
	
	result = num + num2;
	document.write(result + '<br/>');
	
	result = num - num2
	document.write(result + '<br/>');
	
	result = num * num2
	document.write(result+ '<br/>');
	
	result = num / num2
	document.write(result + '<br/>');
	
	result = num % num2
	document.write(result + '<br/>');
	


</script>

